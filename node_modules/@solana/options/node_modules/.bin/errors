#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/-S" ]; then
  "$basedir/-S"  node "$basedir/../../../errors/bin/cli.mjs" "$@"
  ret=$?
else 
  -S  node "$basedir/../../../errors/bin/cli.mjs" "$@"
  ret=$?
fi
exit $ret
